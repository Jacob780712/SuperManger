<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.bc.business.po.MchValueList" table="mch_value_list" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="mch_number" type="java.lang.String">
            <column name="mch_number"/>
        </property>
        <property name="mch_name" type="java.lang.String">
            <column name="mch_name"/>
        </property>
        <property name="ck_id" type="java.lang.String">
            <column name="ck_id"/>
        </property>
        <property name="ck_name" type="java.lang.String">
            <column name="ck_name"/>
        </property>
         <property name="ck_quate" type="java.lang.String">
            <column name="ck_quate"/>
        </property>
        <property name="sale_amount" type="java.lang.String">
            <column name="sale_amount"/>
        </property>
        <property name="card_number" type="java.lang.String">
            <column name="card_number"/>
        </property>
        <property name="all_ck_quate" type="java.lang.String">
            <column name="all_ck_quate"/>
        </property>
        <property name="all_sale_amont" type="java.lang.String">
            <column name="all_sale_amont"/>
        </property>
        <property name="card_balance" type="java.lang.String">
            <column name="card_balance" />
        </property>
        <property name="total_actrul_amount" type="java.lang.String">
            <column name="total_actrul_amount"/>
        </property>
        <property name="create_date" type="java.util.Date">
            <column name="create_date"/>
        </property>
    </class>
</hibernate-mapping>
