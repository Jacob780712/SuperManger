<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.bc.mchant.po.MchCardKinds" table="mch_card_kinds" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="ck_name" type="java.lang.String">
            <column name="ck_name"/>
        </property>
        <property name="ck_type" type="java.lang.String">
            <column name="ck_type"/>
        </property>
        <property name="ck_quota" type="java.lang.Integer">
            <column name="ck_quota" />
        </property>
        <property name="sales_amount" type="java.lang.Integer">
            <column name="sales_amount" />
        </property>
        <property name="discount" type="java.lang.String">
            <column name="discount" />
        </property>
        <property name="period_validity" type="java.lang.String">
            <column name="period_validity"/>
        </property>
        <property name="mch_number" type="java.lang.String">
            <column name="mch_number"/>
        </property>
        <property name="mch_name" type="java.lang.String">
            <column name="mch_name"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"/>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"/>
        </property>
        <property name="create_date" type="java.util.Date">
            <column name="create_date"/>
        </property>
        <property name="create_person" type="java.lang.String">
            <column name="create_person"/>
        </property>
        <property name="update_date" type="java.util.Date">
            <column name="update_date" />
        </property>
        <property name="update_person" type="java.lang.String">
            <column name="update_person"/>
        </property>
    </class>
</hibernate-mapping>
