<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.bc.mchant.po.MchAccountNumber" table="mch_account_number" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="mch_number" type="java.lang.String">
            <column name="mch_number"/>
        </property>
        <property name="account_number" type="java.lang.String">
            <column name="account_number"/>
        </property>
        <property name="account_pass" type="java.lang.String">
            <column name="account_pass"/>
        </property>
        <property name="authority" type="java.lang.String">
            <column name="authority"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"/>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"/>
        </property>
        <property name="create_date" type="java.util.Date">
            <column name="create_date"/>
        </property>
        <property name="create_person" type="java.lang.String">
            <column name="create_person"/>
        </property>
        <property name="update_date" type="java.util.Date">
            <column name="update_date" />
        </property>
        <property name="update_person" type="java.lang.String">
            <column name="update_person"/>
        </property>
        <property name="branch_id" type="java.lang.Integer">
            <column name="branch_id"/>
        </property>
        <property name="source" type="java.lang.String">
            <column name="source"/>
        </property>
    </class>
</hibernate-mapping>
