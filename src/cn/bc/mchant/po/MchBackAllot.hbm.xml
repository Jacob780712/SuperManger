<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.bc.mchant.po.MchBackAllot" table="mch_back_allot" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="mch_number" type="java.lang.String">
            <column name="mch_number"/>
        </property>
        <property name="a_allot" type="java.lang.String">
            <column name="a_allot"/>
        </property>
        <property name="b_allot" type="java.lang.String">
            <column name="b_allot"/>
        </property>
        <property name="o_allot" type="java.lang.String">
            <column name="o_allot"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"/>
        </property>
        <property name="create_date" type="java.util.Date">
            <column name="create_date"/>
        </property>
        <property name="create_person" type="java.lang.String">
            <column name="create_person"/>
        </property>
        <property name="update_date" type="java.util.Date">
            <column name="update_date" />
        </property>
        <property name="update_person" type="java.lang.String">
            <column name="update_person"/>
        </property>
    </class>
</hibernate-mapping>
