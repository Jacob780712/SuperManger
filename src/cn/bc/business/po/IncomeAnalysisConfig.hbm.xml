<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.bc.business.po.IncomeAnalysisConfig" table="income_analysis_config" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="ranges" type="java.lang.Integer">
            <column name="ranges"/>
        </property>
        <property name="create_date" type="java.util.Date">
            <column name="create_date"/>
        </property>
        <property name="create_person" type="java.lang.String">
            <column name="create_person"/>
        </property>
        <property name="update_date" type="java.util.Date">
            <column name="update_date" />
        </property>
        <property name="update_person" type="java.lang.String">
            <column name="update_person"/>
        </property>
    </class>
</hibernate-mapping>
