<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.bc.settlement.po.StmWechat" table="stm_wechat" >
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="date" type="java.util.Date">
            <column name="date"/>
        </property>
        <property name="business_pen" type="java.lang.String">
            <column name="business_pen"/>
        </property>
        <property name="business_amount" type="java.lang.Integer">
            <column name="business_amount"/>
        </property>
        <property name="business_fee" type="java.lang.Integer">
            <column name="business_fee"/>
        </property>
        <property name="settlement_amount" type="java.lang.Integer">
            <column name="settlement_amount"/>
        </property>
        <property name="aut_business_fee" type="java.lang.Integer">
            <column name="aut_business_fee"/>
        </property>
        <property name="aut_settlement_amount" type="java.lang.Integer">
            <column name="aut_settlement_amount"/>
        </property>
         <property name="batch_number" type="java.lang.String">
            <column name="batch_number"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"/>
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark"/>
        </property>
        <property name="create_date" type="java.util.Date">
            <column name="create_date"/>
        </property>
        <property name="create_person" type="java.lang.String">
            <column name="create_person"/>
        </property>
        <property name="update_date" type="java.util.Date">
            <column name="update_date" />
        </property>
        <property name="update_person" type="java.lang.String">
            <column name="update_person"/>
        </property>
    </class>
</hibernate-mapping>
